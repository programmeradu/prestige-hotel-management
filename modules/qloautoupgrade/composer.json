{
  "name": "qloapps/qloautoupgrade",
  "description": "QloApps 1-Click Upgrade",
  "license": "AFL-3.0",
  "authors": [
    {
      "name": "Webkul",
      "email": "support@webkul.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-zip": "*",
    "symfony/filesystem": "~2.8",
    "doctrine/collections": "~1.3.0",
    "twig/twig": "^1.35"
  },
  "config": {
    "preferred-install": "dist",
    "platform": {
        "php": "5.6"
    },
    "prepend-autoloader": false,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "QloApps\\Module\\AutoUpgrade\\": "classes/",
      "QloApps\\Module\\AutoUpgrade\\Temp\\": "templates/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5"
  }
}
